{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "com.kyoden.bomtool",
  "productName": "Kyoden BOM Tool",
  "version": "0.1.0",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "beforeBundleCommand": null,
    "devUrl": null,
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null,
      "capabilities": [
        "default"
      ]
    },
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "Kyoden BOM Tool - 部品表比較・変換・合成ツール",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": []
  },
  "plugins": {}
}
